# FILE : MAKEFILE.UNI

!include <ntwin32.mak>

#include support for unicode
cflags = $(cflags) -D_UNICODE -DUNICODE
midlflags = -D _UNICODE

#include library for CommandLineToArgvW function
conlibsdll = $(conlibsdll) shell32.lib

!include <makefile>
